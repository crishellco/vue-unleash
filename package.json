{
  "name": "vue-unleash",
  "version": "0.3.0",
  "author": "Chris Mitchell (@crishellco)",
  "description": "A Vue & Vuex plugin for the Unleash open-source feature flag platform",
  "scripts": {
    "build": "node_modules/.bin/vue-cli-service build",
    "test": "node_modules/.bin/vue-cli-service test:unit && jest-coverage-badges output badges",
    "lint": "node_modules/.bin/vue-cli-service lint"
  },
  "main": "dist/vue-unleash.common.js",
  "module": "dist/vue-unleash.esm.js",
  "unpkg": "dist/vue-unleash.umd.min.js",
  "files": [
    "dist/vue-unleash.common.js",
    "dist/vue-unleash.umd.min.js",
    "dist/vue-unleash.umd.js",
    "dist/vue-unleash.esm.js",
    "src"
  ],
  "dependencies": {},
  "devDependencies": {
    "@vue/cli": "^4.1.1",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-unit-jest": "^4.1.1",
    "@vue/cli-plugin-vuex": "^4.1.1",
    "@vue/cli-service": "^4.1.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "fetch-mock": "^8.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jest-coverage-badges": "^1.1.2",
    "lodash": "^4.17.15",
    "vue": "^2.6.10",
    "vue-cli-plugin-p11n": "^0.4.0",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.2",
    "zl-fetch": "^3.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "homepage": "https://github.com/crishellco/vue-unleash#readme",
  "jsdelivr": "dist/vue-unleash.umd.min.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crishellco/vue-unleash.git"
  }
}
